#include <stdio.h>

int main() {
    int n, i;
    int fib0 = 1, fib1 = 1, fib_next;

    printf("Ingrese el valor de n: ");
    scanf("%d", &n);

    if (n == 0) {
        printf("El numero de Fibonacci en la posicion %d es: %d\n", n, fib0);
    } else if (n == 1) {
        printf("El numero de Fibonacci en la posicion %d es: %d\n", n, fib1);
    } else {
        for (i = 2; i <= n; i++) {
            fib_next = fib0 + fib1;
            fib0 = fib1;
            fib1 = fib_next;
        }
        printf("El numero de Fibonacci en la posicion %d es: %d\n", n, fib1);
    }

    return 0;
}
