#include <stdio.h>

int main() {
    int i, numero, suma = 0;

    printf("Ingrese un numero entero: ");
    scanf("%d", &numero);

    for (i = 1; i <= numero; i++) {
        if (i % 2 != 0) {
            suma += i;}
    }
    printf("\nLa suma de los numeros impares entre 1 y %d es: %d\n", numero, suma);
    return 0;
}
