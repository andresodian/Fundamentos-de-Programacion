#include <stdio.h>

int main() {
    int h1, m1, s1;
    int h2, m2, s2;
    int t1, t2, dif;
    int rh, rm, rs;

    printf("Ingrese el primer horario (hh mm ss): ");
    scanf("%d %d %d", &h1, &m1, &s1);

    printf("Ingrese el segundo horario (hh mm ss): ");
    scanf("%d %d %d", &h2, &m2, &s2);

    t1 = h1*3600 + m1*60 + s1;
    t2 = h2*3600 + m2*60 + s2;

    if (t2 < t1) {
        printf("Error: el segundo horario debe ser mayor que el primero.\n");
        return 0;
    }

    dif = t2 - t1;

    rh = dif / 3600;
    dif = dif % 3600;
    rm = dif / 60;
    rs = dif % 60;

    printf("Tiempo transcurrido: %d:%d:%d\n", rh, rm, rs);

    return 0;
}
