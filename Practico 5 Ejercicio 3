#include <stdio.h>
int main () {
int N, u, d, c, m;

printf("Ingresa un numero entero:");
scanf("%d",&N);

if (N>=0 && N<10000) {
    u = N % 10;
    d = (N /10) % 10;
    c = (N /100) % 10;
    m = (N /1000) % 10;
    printf("Unidad: %d\n" ,u);
    printf("Decena: %d\n" ,d);
    printf("Centena: %d\n" ,c);
    printf("Unidad de mil: %d\n", m);
} else {
      printf("El numero debe ser mayor a 0 y menor a 10000");
      }
return 0;
}
